{
  "name": "UCDP Georeferenced Event Dataset (GED)",
  "key": "ged",
  "description": "This dataset is UCDP's most disaggregated dataset, covering individual events of organized violence (phenomena of lethal violence occurring at a given time and place).",
  "interval": "1989 - 2017",
  "codebook": "http://ucdp.uu.se/downloads/ged/ged181.pdf",
  "download": "http://ucdp.uu.se/downloads/ged/ged181-csv.zip",
  "host": "TwoRavens",
  "subsets": {
    "Actor": {
		"measureType": "unit",
		"type": "dyad",
		"tabs": {
			"source": {
				"full": "TwoRavens_country_src",
				"filters": []
			},
			"target": {
				"full": "TwoRavens_country_tgt",
				"filters": []
			}
		}
	},
	"Date": {
		"type": "date",
		"measureType": "unit",
		"structure": "point",
		"columns": ["TwoRavens_start date"]
	},
	"Location": {
		"type": "discrete_grouped",
		"group_by": "region",
		"columns": ["TwoRavens_country"]
	},
	"Coordinates": {
		"type": "coordinates",
		"columns": [
			"latitude",
			"longitude"
		]
	}
  },
  "columns": [
    "id",
    "year",
    "active_year",
    "type_of_violence",
    "confict_new_id",
    "conflict_name",
    "dyad_new_id",
    "dyad_name",
    "side_a_new_id",
    "side_a",
    "side_b_new_id",
    "side_b",
    "number_of_sources",
    "source_article",
    "source_original",
    "where_prec",
    "where_coordinates",
    "adm_1",
    "latitude",
    "longitude",
    "geom_wkt",
    "priogrid_gid",
    "country",
    "country_id",
    "region",
    "event_clarity",
    "date_prec",
    "date_start",
    "date_end",
    "deaths_a",
    "deaths_b",
    "deaths_civilians",
    "deaths_unknown",
    "low",
    "best",
    "high"
  ],
  "columns_constructed": [
    "TwoRavens_start date",
    "TwoRavens_end date",
    "TwoRavens_date info",
    "TwoRavens_country",
    "TwoRavens_address",
    "TwoRavens_city",
    "TwoRavens_postal",
    "TwoRavens_postal_ext",
    "TwoRavens_region",
    "TwoRavens_subregion",
    "TwoRavens_country_historic",
    "TwoRavens_country_src",
    "TwoRavens_country_historic_src",
    "TwoRavens_country_tgt",
    "TwoRavens_country_historic_tgt"
  ],
  "alignments": {
	  "TwoRavens_start date": "date",
	  "latitude": "latitude",
	  "longitude": "longitude",
	  "TwoRavens_country": "country"
  },
  "formats": {
	  "TwoRavens_country": "ISO-3"
  },
