{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "data": {"url": "tworavensData"},
  "description": "An interactive scatterplot for three quantitative variables.",
  "selection": {
    "grid": {
      "type": "interval", "bind": "scales"
    }
  },
  "mark": "circle",
  "encoding": {
    "x": {
      "field": "tworavensX", "type": "quantitative", "scale": {"zero": false}
    },
    "y": {
      "field": "tworavensY", "type": "quantitative", "scale": {"zero": false}
    },
    "size": {"field": "tworavensZ", "type": "quantitative"}
  }
}
