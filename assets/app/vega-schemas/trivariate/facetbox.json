{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "faceted boxplot",
  "data": {
    "url": "tworavensData"
  },
  "facet": {
    "row": {
      "field": "tworavensZ",
      "type": "nominal"
    }
  },
  "spec":{
  "transform": [
    {
      "aggregate": [
        {
          "op": "q1",
          "field": "tworavensY",
          "as": "lowerBox"
        },
        {
          "op": "q3",
          "field": "tworavensY",
          "as": "upperBox"
        },
        {
          "op": "median",
          "field": "tworavensY",
          "as": "midBox"
        },
        {
          "op": "min",
          "field": "tworavensY",
          "as": "lowerWhisker"
        },
        {
          "op": "max",
          "field": "tworavensY",
          "as": "upperWhisker"
        }
      ],
      "groupby": [
        "tworavensX"
      ]
    }
  ],
  "layer": [
    {
      "mark": {
        "type": "rule",
        "style": "boxWhisker"
      },
      "encoding": {
        "y": {
          "field": "lowerWhisker",
          "type": "quantitative",
          "axis": {
            "title": "tworavensY"
          }, "scale": {"zero":  false}
        },
        "y2": {
          "field": "lowerBox",
          "type": "quantitative"
        },
        "x": {
          "field": "tworavensX",
          "type": "nominal"
        }
      }
    },
    {
      "mark": {
        "type": "rule",
        "style": "boxWhisker"
      },
      "encoding": {
        "y": {
          "field": "upperBox",
          "type": "quantitative",
          "scale": {"zero":  false}
        },
        "y2": {
          "field": "upperWhisker",
          "type": "quantitative"
        },
        "x": {
          "field": "tworavensX",
          "type": "nominal"
        }
      }
    },
    {
      "mark": {
        "type": "bar",
        "style": "box"
      },
      "encoding": {
        "y": {
          "field": "lowerBox",
          "type": "quantitative",
          "scale": {"zero":  false}
        },
        "y2": {
          "field": "upperBox",
          "type": "quantitative"
        },
        "x": {
          "field": "tworavensX",
          "type": "nominal"
        },
        "size": {
          "value": 5
        }
      }
    },
    {
      "mark": {
        "type": "tick",
        "style": "boxMid"
      },
      "encoding": {
        "y": {
          "field": "midBox",
          "type": "quantitative",
          "scale": {"zero":  false}
        },
        "x": {
          "field": "tworavensX",
          "type": "nominal"
        },
        "color": {
          "value": "white"
        },
        "size": {
          "value": 5
        }
      }
    }
  ]
  }
}
