{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "A stacked bar chart.",
  "data": {
    "url": "tworavensData"
  },
  "mark": "bar",
  "encoding": {
    "x": {
      "timeUnit": null,
      "field": "tworavensX",
      "type": "nominal",
      "axis": {
        "title": "tworavensX"
      }
    },
    "y": {
      "field": "tworavensY",
      "aggregate": "count",
      "type": "nominal",
      "title": "tworavensY"
    },
    "color": {
      "field": "tworavensY",
      "type": "nominal",
      "scale": {
        "domain": "tworavensUniqueY",
        "range": "tworavensColors"
      },
      "legend": {
        "title": "tworavensY"
      }
    },
    "row": {
      "field": "tworavensZ",
      "type": "nominal"
    }
  }
}
