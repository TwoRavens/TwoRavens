  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "data": {"url": "tworavensData"},
  "description": "A horizontal grouped bar chart.",
  "mark": "bar",
  "encoding": {
    "x": {"aggregate": "sum", "field": "tworavensX", "type": "quantitative"},
