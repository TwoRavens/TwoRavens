{
  "$schema": "https://vega.github.io/schema/vega-lite/v3.json",
  "description": "A scatterplot.",
  "title": "tworavensTitle",
  "data": {
    "url": "tworavensData"
  },
  "autosize": {
    "type": "fit",
    "contains": "padding"
  },
  "mark": "point",
  "encoding": {
    "x": {"field": "tworavensX", "type": "quantitative"},
    "y": {"field": "tworavensY", "type": "quantitative"},
    "color": {"field": "tworavensGroup", "type": "nominal"},
    "tooltip": {"field": "tworavensLabel", "type": "nominal"}
  }
}