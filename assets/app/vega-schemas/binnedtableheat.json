{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "A binned heatmap table.",
  "data": {"url": "tworavensData"},
  "mark": "rect",
  "encoding": {
    "x": {
      "bin": {"maxbins":20},
      "field": "tworavensX",
      "type": "quantitative",
      "scale": {"zero": false}
    },
    "y": {
      "bin": {"maxbins": 20},
      "field": "tworavensY",
      "type": "quantitative",
      "scale": {"zero": false}
    },
    "color": {
      "aggregate": "count",
      "type": "quantitative"
    },
    "tooltip": [
      {"field": "tworavensX","type": "quantitative", "bin":  {"maxbins": 20}},
      {"field": "tworavensY","type": "quantitative", "bin":  {"maxbins": 20}},
      {"aggregate":  "count", "type": "quantitative"}
    ]
  },
  "config": {
    "range": {
      "heatmap": {
        "scheme": "greenblue"
      }
    },
    "view": {
      "stroke": "transparent"
    }
  }
}
