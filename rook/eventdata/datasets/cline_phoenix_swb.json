{
  "name": "Cline - BBC Summary",
  "key": "cline_phoenix_swb",
  "description": "Data sourced from the BBC Summary of World Broadcasts and collected by the Cline Center for Advanced Social Research.",
  "interval": "1979 - 2015",
  "host": "TwoRavens",
  "subsets": {
    "Actor": {
      "type": "dyad",
      "tabs": {
        "source": {
          "full": "source",
          "filters": [
            "source_agent",
            "source_others",
            "source_root"
          ]
        },
        "target": {
          "full": "target",
          "filters": [
            "target_agent",
            "target_others",
            "target_root"
          ]
        }
      },
      "format": "phoenix",
      "search_length": 3,
      "deconstruct": {
        "source_others": ";",
        "target_others": ";"
      }
    },
    "Action": {
      "type": "action",
      "format": "CAMEO root code",
      "columns": [
        "root_code"
      ]
    },
    "Date": {
      "type": "date",
      "format": "fields",
      "columns": [
        "year",
        "month"
      ]
    },
    "Location": {
      "type": "location",
      "format": "ISO-3",
      "columns": [
        "countryname"
      ]
    },
    "Coordinates": {
      "type": "coordinates",
      "format": "signed degrees",
      "columns": []
    }
  },
  "columns": {
    "aid": "<id>",
    "code": "<cameo>",
    "countryname": "<country>",
    "day": "<day>",
    "eid": "<event_id>",
    "goldstein": "<goldstein>",
    "lat": "<latitude>",
    "lon": "<longitude>",
    "month": "<month>",
    "placename": "<geoname>",
    "process": "<process>",
    "quad_class": "<quad_class>",
    "root_code": "<action_code>",
    "source": "<source>",
    "source_agent": "<src_sector>",
    "source_others": "<src_other_sector>",
    "source_root": "<src_country>",
    "statename": "<admin_info>",
    "story_date": "<date>",
    "target": "<target>",
    "target_agent": "<tgt_sector>",
    "target_others": "<tgt_other_sector>",
    "target_root": "<tgt_country>",
    "year": "<year>"
  },
  "citations": [
    {
      "author": "Althaus, Scott, Joseph Bajjalieh, John F. Carter, Buddy Peyton, and Dan A. Shalmon",
      "year": "2017",
