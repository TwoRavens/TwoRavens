    "target",
    "target_agent",
    "target_others",
    "target_root",
    "year"
  ],
  "columns_constructed": [
    "story_date_constructed"
  ],
  "alignments": {
    "code": "action",
    "quad_class": "action",
    "root_code": "action",
    "source": "sector",
    "source_agent": "country",
    "source_root": "sector",
    "target": "sector",
    "target_agent": "country",
    "target_root": "sector",
    "story_date_constructed": "date",
    "lat": "latitude",
    "lon": "longitude",
    "country_constructed": "country"
  },
  "formats": {
    "code": "CAMEO",
    "quad_class": "penta_class",
    "root_code": "root_code",
    "source": "phoenix",
