{
  "name": "Cline - BBC Summary",
  "key": "cline_phoenix_swb",
  "description": "Data sourced from the BBC Summary of World Broadcasts and collected by the Cline Center for Advanced Social Research.",
  "interval": "1979 - 2015",
  "codebook": "https://uofi.app.box.com/s/bmh9i39m6bf0vhnuebtf3ak3j6uxy2le",
  "host": "TwoRavens",
  "subsets": {
    "Actor": {
      "type": "dyad",
      "measures": [
        "unit"
      ],
      "tabs": {
        "source": {
          "full": "source",
          "filters": [
            "source_agent",
            "source_others",
            "source_root"
          ],
          "token_length": 3
        },
        "target": {
          "full": "target",
          "filters": [
            "target_agent",
            "target_others",
            "target_root"
          ],
          "token_length": 3
        }
      }
    },
    "Action": {
      "type": "categorical",
      "measures": [
        "event"
      ],
      "formats": [
        "CAMEO",
        "root_code",
        "penta_class",
        "PLOVER"
      ],
      "columns": [
        "code"
      ]
    },
    "Date": {
      "type": "date",
      "measures": [
        "unit"
      ],
      "structure": "point",
      "columns": [
        "story_date_constructed"
      ]
    },
    "Location": {
      "type": "categorical_grouped",
      "group_by": "region",
      "columns": [
        "country_constructed"
      ]
    },
    "Coordinates": {
      "type": "coordinates",
      "columns": [
        "lat",
        "lon"
      ]
    }
  },
  "columns": [
    "aid",
    "code",
    "countryname",
    "day",
    "eid",
    "goldstein",
    "lat",
    "lon",
    "month",
