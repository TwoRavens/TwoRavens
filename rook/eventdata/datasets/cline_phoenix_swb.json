{
  "name": "Cline - BBC Summary",
  "key": "cline_phoenix_swb",
  "description": "Data sourced from the BBC Summary of World Broadcasts and collected by the Cline Center for Advanced Social Research.",
  "interval": "1979 - 2015",
  "codebook": "https://uofi.app.box.com/s/bmh9i39m6bf0vhnuebtf3ak3j6uxy2le",
  "host": "TwoRavens",
  "subsets": {
    "Actor": {
      "type": "dyad",
      "alignment": "actor",
      "formats": ["phoenix"],
      "tabs": {
        "source": {
          "full": "source",
          "filters": [
            "source_agent",
            "source_others",
            "source_root"
          ],
          "token_length": 3,
          "deconstruct": {
            "source_others": ";"
          }
        },
        "target": {
          "full": "target",
          "filters": [
            "target_agent",
            "target_others",
            "target_root"
          ],
          "token_length": 3,
          "deconstruct": {
            "target_others": ";"
          }
        }
      }
    },
    "Action": {
      "type": "categorical",
      "alignment": "action",
      "formats": ["CAMEO", "root_code", "penta_class"],
      "columns": [
        "code"
      ]
    },
    "Date": {
      "type": "date",
      "columns": [
        "date_event_constructed"
      ]
    },
    "Location": {
      "type": "categorical_grouped",
      "alignment": "location",
      "group_by": "region",
      "formats": ["ISO-3"],
      "columns": [
        "countryname_constructed"
      ]
    },
    "Coordinates": {
      "type": "coordinates",
      "format": "signed degrees",
      "columns": [
        "lat",
        "lon"
      ]
    }
  },
  "columns": [
    "aid",
    "code",
    "countryname",
    "day",
    "eid",
    "goldstein",
    "lat",
    "lon",
    "month",
    "placename",
    "process",
    "quad_class",
    "root_code",
    "source",
    "source_agent",
    "source_others",
    "source_root",
    "statename",
    "story_date",
    "target",
    "target_agent",
    "target_others",
    "target_root",
    "year"
  ],
  "columns_constructed": [
    "story_date_constructed"
  ],
  "ontology_alignments": {
    "code": "<cameo>",
    "countryname": "<country>",
    "goldstein": "<goldstein>",
    "lat": "<latitude>",
    "lon": "<longitude>",
    "placename": "<geoname>",
    "quad_class": "<quad_class>",
    "root_code": "<root_code>",
    "source_agent": "<src_sector>",
    "source_root": "<src_country>",
    "story_date_constructed": "<date>",
    "target_agent": "<tgt_sector>",
    "target_root": "<tgt_country>"
  },
  "citations": [
    {
      "author": "Althaus, Scott, Joseph Bajjalieh, John F. Carter, Buddy Peyton, and Dan A. Shalmon",
      "year": "2017",
      "title": "Cline Center Historical Phoenix Event Data",
      "version": "v.1.0.0",
      "note": "Distributed by Cline Center for Advanced Social Research",
      "date": "June 30",
      "url": "http://www.clinecenter.illinois.edu/data/event/phoenix/"
    },
    {
      "author": "Althaus, Scott, Joseph Bajjalieh, John F. Carter, Buddy Peyton, and Dan A. Shalmon",
      "year": "2017",
      "description": "Cline Center Historical Phoenix Event Data Variable Descriptions",
      "title": "Cline Center Historical Phoenix Event Data",
      "version": "v.1.0.0",
      "note": "Cline Center for Advanced Social Research",
      "date": "June 30",
      "url": "http://www.clinecenter.illinois.edu/data/event/phoenix/"
    }
  ]
}
