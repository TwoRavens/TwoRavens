{
  "name": "UTDallas - Real Time Phoenix Data",
  "key": "phoenix_rt",
  "description": "A real-time Phoenix-coded event dataset constructed at The University of Texas at Dallas.",
  "interval": "Oct. 2017 - present",
  "host": "UTDallas",
  "subsets": {
    "Actor": {
      "type": "dyad",
      "tabs": {
        "source": {
          "format": "phoenix",
          "full": "source",
          "filters": [
            "src_agent",
            "src_other_agent",
            "src_actor"
          ],
          "token_length": 3,
          "deconstruct": [
            "src_other_agent"
          ]
        },
        "target": {
          "format": "phoenix",
          "full": "target",
          "filters": [
            "tgt_agent",
            "tgt_other_agent",
            "tgt_actor"
          ],
          "token_length": 3,
          "deconstruct": [
            "tgt_other_agent"
          ]
        }
      }
    },
    "Action": {
      "type": "action",
      "format": "CAMEO root code",
      "columns": [
        "root code"
      ]
    },
    "Date": {
      "type": "date",
      "format": "YYYYMMDD",
      "columns": [
        "date8"
      ]
    },
    "Location": {
      "type": "location",
      "format": "ISO-3",
      "columns": [
        "country_code"
      ]
    },
    "Coordinates": {
      "type": "coordinates",
      "format": "signed degrees",
      "columns": []
    }
  },
  "columns": [
    "code",
    "country_code",
    "date8",
    "day",
    "geoname",
    "goldstein",
    "id",
    "latitude",
    "longitude",
    "month",
    "quad_class",
    "root_code",
    "source_text",
    "source",
    "src_actor",
    "src_agent",
    "src_other_agent",
    "target",
    "tgt_actor",
    "tgt_agent",
    "tgt_other_agent",
    "url",
    "year"
  ],
  "alignments": {
    "latitude": "latitude",
    "longitude": "longitude"
  },
  "citations": [
    {
      "title": "Phoenix real time event data",
      "author": "Brandt, Patrick T.; “D’Orazio, Vito; Holmes, Jennifer; Khan, Latifur; Ng, Vincent",
      "year": "2018",
      "note": "University of Texas Dallas",
      "url": "http://eventdata.utdallas.edu"
    }
  ]
}
