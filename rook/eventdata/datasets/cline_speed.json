{
  "name": "Cline - Speed",
  "key": "cline_speed",
  "description": "Data sourced from The Social, Political and Economic Event Database Project and made available by the Cline Center for Advanced Social Research.",
  "interval": "1979 - 2015",
  "host": "TwoRavens",
  "subsets": {
    "Actor": {
      "type": "dyad",
      "tabs": {
        "source": {
          "format": "phoenix",
          "full": "source",
          "filters": [
            "source_agent",
            "source_others",
            "source_root"
          ],
          "token_length": 3,
          "deconstruct": {
            "source_others": ";"
          }
        },
        "target": {
          "format": "phoenix",
          "full": "target",
          "filters": [
            "target_agent",
            "target_others",
            "target_root"
          ],
          "token_length": 3,
          "deconstruct": {
            "target_others": ";"
          }
        }
      }
    },
    "Action": {
      "type": "action",
      "format": "CAMEO root code",
      "columns": [
        "root_code"
      ]
    },
    "Date": {
      "type": "date",
      "format": "fields",
      "columns": [
        "year",
        "month"
      ]
    },
    "Location": {
      "type": "location",
      "format": "ISO-3",
      "columns": [
        "countryname"
      ]
    },
    "Coordinates": {
      "type": "coordinates",
      "format": "signed degrees",
      "columns": []
    }
  },
