{
  "name": "ICEWS",
  "key": "icews",
  "description": "Event data consisting of coded interactions between socio-political actors (i.e., cooperative or hostile actions between individuals, groups, sectors and nation states).",
  "interval": "1995 - Sep. 2016",
  "codebook": "https://dataverse.harvard.edu/file.xhtml?fileId=2548466&version=RELEASED&version=.0",
  "host": "TwoRavens",
  "subsets": {
    "Actor": {
      "type": "dyad",
      "tabs": {
        "source": {
          "full": "Source Name",
          "filters": [
            "Source Sectors",
            "Source Country"
          ]
        },
        "target": {
          "full": "Target Name",
          "filters": [
            "Target Sectors",
            "Target Country"
          ]
        }
      }
    },
    "Action": {
      "type": "categorical",
      "alignments": [
        "action"
      ],
      "formats": [
        "CAMEO",
        "root_code",
        "penta_class",
        "PLOVER"
      ],
      "columns": [
        "CAMEO Code"
      ]
    },
    "Date": {
      "type": "date",
      "structure": "point",
      "columns": [
        "Event Date_constructed"
      ]
    },
    "Location": {
      "type": "categorical_grouped",
      "group_by": "region",
      "formats": [
        "ISO-3"
      ],
      "columns": [
        "Country_constructed"
      ]
    },
    "Coordinates": {
      "type": "coordinates",
      "format": "signed degrees",
      "columns": [
        "Latitude",
        "Longitude"
      ]
    }
  },
  "columns": [
    "CAMEO Code",
    "City",
    "Country",
    "District",
    "Event Date",
    "Event ID",
    "Event Text",
    "Intensity",
    "Latitude",
    "Longitude",
    "Province",
    "Publisher",
    "Sentence Number",
    "Source Country",
    "Source Name",
    "Source Sectors",
    "Story ID",
    "Target Country",
    "Target Name",
    "Target Sectors"
  ],
  "columns_constructed": [
    "Event Date_constructed",
    "Country_constructed"
  ],
  "alignments": {
    "Event Date_constructed": "date",
    "Latitude": "latitude",
    "Longitude": "longitude"
  },
  "formats": {
    "Source Sectors": "icews",
    "Target Sectors": "icews",
    "CAMEO Code": "CAMEO"
  },
  "citations": [
    {
      "author": "Boschee, Elizabeth; Lautenschlager, Jennifer; O'Brien, Sean; Shellman, Steve; Starz, James; Ward, Michael",
      "year": "2015",
      "title": "ICEWS Coded Event Data",
      "url": "https://doi.org/10.7910/DVN/28075",
      "version": "Harvard Dataverse, V22"
    }
  ]
