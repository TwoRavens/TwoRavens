{
  "name": "ICEWS",
  "key": "icews",
  "description": "Event data consists of coded interactions between socio-political actors (i.e., cooperative or hostile actions between individuals, groups, sectors and nation states).",
  "interval": "1995 - Sep. 2016",
  "host": "TwoRavens",
  "subsets": {
    "Actor": {
      "type": "dyad",
      "tabs": {
        "source": {
          "format": "icews",
          "full": "Source Name",
          "filters": [
            "Source Sectors",
            "Source Country"
          ]
        },
        "target": {
          "format": "icews",
          "full": "Target Name",
          "filters": [
            "Target Sectors",
            "Target Country"
          ]
        }
      }
    },
    "Action": {
      "type": "action",
      "format": "CAMEO",
      "columns": [
        "CAMEO Code"
      ]
    },
    "Date": {
      "type": "date",
      "format": "YYYY-MM-DD",
      "columns": [
        "Event Date"
      ]
    },
    "Location": {
      "type": "location",
      "format": "fullname",
      "columns": [
        "Country"
      ]
    },
    "Coordinates": {
      "type": "coordinates",
      "format": "signed degrees",
      "columns": []
    }
  },
  "columns": {
    "CAMEO Code": "<cameo>",
    "City": "<city>",
    "Country": "<country>",
    "District": "<district>",
    "Event Date": "<date>",
    "Event ID": "<event_id>",
    "Event Text": "<event_text>",
    "Intensity": "<goldstein>",
    "Latitude": "<latitude>",
    "Longitude": "<longitude>",
    "Province": "<province>",
    "Publisher": "<publisher>",
    "Sentence Number": "<sentence_number>",
    "Source Country": "<src_country>",
    "Source Name": "<source>",
    "Source Sectors": "<src_sector>",
    "Story ID": "<id>",
    "Target Country": "<tgt_country>",
    "Target Name": "<target>",
    "Target Sectors": "<tgt_sector>"
  },
  "citations": [
    {
      "author": "Boschee, Elizabeth; Lautenschlager, Jennifer; O'Brien, Sean; Shellman, Steve; Starz, James; Ward, Michael",
      "year": "2015",
      "title": "ICEWS Coded Event Data",
      "url": "https://doi.org/10.7910/DVN/28075",
      "version": "Harvard Dataverse, V22"
    }
  ]
}
